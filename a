echo -e "\n16 Basic Colors:"
for c in {0..15}; do
    echo -en "\e[48;5;${c}m  ${c}  \e[0m "
done
echo -e "\n\n216 Extended Colors:"
for c in {16..231}; do
    echo -en "\e[48;5;${c}m ${c} \e[0m"
    [ $(((c-15) % 6)) -eq 0 ] && echo
done
echo -e "\n24 Grayscale:"
for c in {232..255}; do
    echo -en "\e[48;5;${c}m ${c} \e[0m"
done
echo -e "\e[0m\n"
